<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Protected</customSettingsVisibility>
    <description>The list of Statuses that are considered as &quot;Internal Status&quot;. Related to the Case Age in Business Hours app, moving away from a Custom Object and using Custom Setting to leverage caching, and not worry about SOQL queries.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Included_in_AHT__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Agent Handle Time. The time when an agent is actually working a case, e.g., In Progress</description>
        <externalId>false</externalId>
        <inlineHelpText>If not on this list, defaults to not included in AHT</inlineHelpText>
        <label>Included in AHT</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Included_in_SLA_Time__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>If not on this list, defaults to included in SLA</inlineHelpText>
        <label>Included in SLA Time</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <caseSensitive>true</caseSensitive>
        <description>The exact, *Case Sensitive* Status</description>
        <externalId>false</externalId>
        <inlineHelpText>The exact, *Case Sensitive* Status</inlineHelpText>
        <label>Status</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>Status Time Bucket</label>
</CustomObject>
